        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liyv.taotao.mapper.TaoItemMapper">
    <select id="selectByPrimaryKey" resultType="TaoItem">
        select * from tb_item
        where id=#{itemId}
    </select>
